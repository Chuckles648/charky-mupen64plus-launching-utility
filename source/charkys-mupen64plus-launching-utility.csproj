    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>mupen64plus-ui-run-icon.ico</ApplicationIcon>
    <PlatformTarget>x64</PlatformTarget>
